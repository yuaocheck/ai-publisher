{
  "name": "ai-publisher",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:generate": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID --schema public > src/types/database.types.ts",
    "deploy": "wrangler pages deploy .next/static --project-name ai-publisher",
    "workers:dev": "wrangler dev",
    "workers:deploy": "wrangler deploy",
    "workers:deploy:staging": "wrangler deploy --env staging",
    "workers:deploy:production": "wrangler deploy --env production",
    "workers:logs": "wrangler tail",
    "workers:setup": "wrangler kv:namespace create AI_PUBLISHER_KV",
    "workers:secret": "wrangler secret put",
    "deploy:cloudflare": "npm run workers:deploy:production"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.3.0",
    "@google/generative-ai": "^0.2.1",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/supabase-js": "^2.38.4",
    "@tiptap/extension-image": "^2.1.13",
    "@tiptap/extension-link": "^2.1.13",
    "@tiptap/extension-placeholder": "^2.1.13",
    "@tiptap/pm": "^2.1.13",
    "@tiptap/react": "^2.1.13",
    "@tiptap/starter-kit": "^2.1.13",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.292.0",
    "next": "14.0.3",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.4.1",
    "tailwind-merge": "^2.0.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231218.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "postcss": "^8",
    "supabase": "^1.123.4",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "wrangler": "^3.114.11"
  }
}
